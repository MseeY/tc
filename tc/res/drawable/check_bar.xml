<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item android:id="@android:id/background">
        <shape android:shape="rectangle" >
            <solid android:color="@color/ymx_network_progressbar1" />

            <corners android:radius="@dimen/network_9" />

            <size
                android:height="@dimen/margin_1"
                android:width="@dimen/network_7" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape
                xmlns:android="http://schemas.android.com/apk/res/android"
                android:shape="rectangle" >
                <solid android:color="@color/ymx_network_progressbar2" />

                <corners android:radius="@dimen/margin_1" />

                <size
                    android:height="@dimen/margin_1"
                    android:width="@dimen/network_7" />
            </shape>
        </clip>
    </item>

</layer-list>